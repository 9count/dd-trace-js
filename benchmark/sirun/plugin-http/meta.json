{
  "name": "plugin-http",
  "cachegrind": true,
  "iterations": 10,
  "variants": {
    "client-control": {
      "setup": "bash -c \"nohup node server.js &\"",
      "run": "node client.js",
      "teardown": "bash -c \"kill -9 `cat server.pid`\"",
      "env": {
        "TESTING": "client",
        "CLIENT_USE_TRACER": "0",
        "PORT": "3001"
      }
    },
    "client-with-tracer": {
      "setup": "bash -c \"nohup node server.js &\"",
      "run": "node client.js",
      "teardown": "bash -c \"kill -9 `cat server.pid`\"",
      "env": {
        "TESTING": "client",
        "CLIENT_USE_TRACER": "1",
        "PORT": "3002"
      }
    },
    "server-control": {
      "setup": "bash -c \"sleep 2 && nohup node client.js &\"",
      "run": "node server.js",
      "teardown": "bash -c \"kill -9 `cat client.pid`\"",
      "env": {
        "TESTING": "server",
        "SERVER_USE_TRACER": "0",
        "PORT": "3003"
      }
    },
    "server-with-tracer": {
      "setup": "bash -c \"sleep 2 && nohup node client.js &\"",
      "run": "node server.js",
      "teardown": "bash -c \"kill -9 `cat client.pid`\"",
      "env": {
        "TESTING": "server",
        "SERVER_USE_TRACER": "1",
        "PORT": "3004"
      }
    }
  }
}
